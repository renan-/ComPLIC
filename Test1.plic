programme Test1 {
	/* Declarations */
	entier a b c d ;

	/* Instructions */
	a := 4 ;
	b := 5 ;
	si ( a > b ) alors {
		ecrire a ;
	} sinon {
		ecrire b ;
	}

	c := 10 ;
	d := 10 ;
	si ( c <= d ) alors {
		entier e ;

		e := ( c * d + 10 ) / 11 ;
		si  ( ( e >= c ) et ( ( e - 1 + 1 ) = d ) ) alors {
			ecrire d ;
		} sinon {
			ecrire a ;
		}
	}
}